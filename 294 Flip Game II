class Solution {
    public boolean canWin(String s) {
        if(s == null || s.length() < 2) return false;
        
        for(int i = 1; i < s.length(); i++){
            if(s.charAt(i) == '+' && s.charAt(i - 1) == '+'){
                String t = s.substring(0, i - 1) + "--" + s.substring(i + 1);
                if(!canWin(t)) return true;
            } 
        }     
        return false; 
    }
}

